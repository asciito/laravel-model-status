        "pestphp/pest-plugin-laravel": "^2.2"
    },
    "extras": {
        "laravel": {
            "providers": [
                "Asciito\\LaravelModelStatus\\ModelStatusServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
