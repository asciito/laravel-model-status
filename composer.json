{
    "name": "asciito/laravel-model-status",
    "description": "Simple package to manage the status column of a Model",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Ayax CÃ³rdova",
            "email": "ayax.cordova@aydev.mx"
        }
    ],
    "autoload": {
        "psr-4": {
            "Asciito\\LaravelModelStatus\\": "src/"
        }
